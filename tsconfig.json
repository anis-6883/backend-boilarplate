{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "allowJs": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noUnusedParameters": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": false,
    "sourceMap": true,
    "strict": true,
    "pretty": true,
    "baseUrl": "src",
    "outDir": "dist",
    "noImplicitAny": true,
    "rootDir": "src"
    // "paths": {
    //   "@/*": ["./src/*"],
    //   "@middlewares/*": ["./src/middlewares/*"],
    //   "@controllers/*": ["./src/controllers/*"]
    // }
  },
  "include": ["src/**/*"]
}
